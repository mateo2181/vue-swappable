{
  "name": "vue-swappable",
  "version": "0.2.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "rimraf dist && vue-cli-service build --target lib ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@shopify/draggable": "^1.0.0-beta.8",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "vue-template-compiler": "^2.6.10",
    "webpack-dev-server": "^3.7.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "main": "./dist/vue-swappable.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "description": "### Using [Shopify's draggable library](https://github.com/Shopify/draggable)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateo2181/vue-swappable.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mateo2181/vue-swappable/issues"
  },
  "homepage": "https://github.com/mateo2181/vue-swappable#readme"
}
